{
    "name": "lara-zeus/qr",
    "description": "Zeus Qr Code Input to generate Qr Code with designing options for filamentPHP",
    "keywords": [
        "laravel",
        "lara-zeus",
        "qr",
        "code",
        "design",
        "input",
        "coulmn",
        "field",
        "generator",
        "filamentphp"
    ],
    "homepage": "https://larazeus.com/qr",
    "support": {
        "issues": "https://github.com/lara-zeus/qr/issues",
        "source": "https://github.com/lara-zeus/qr"
    },
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Lara Zeus (Ash)",
            "email": "info@larazeus.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.0",
        "simplesoftwareio/simple-qrcode": "~4",
        "spatie/laravel-package-tools": "^1.16"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.0",
        "nunomaduro/larastan": "^2.0.1",
        "nunomaduro/phpinsights": "^2.8",
        "orchestra/testbench": "^8.0",
        "phpstan/extension-installer": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "LaraZeus\\Qr\\": "src"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "format": "vendor/bin/pint",
        "pint": "vendor/bin/pint",
        "test:phpstan": "vendor/bin/phpstan analyse"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaraZeus\\Qr\\QrServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
